<form action="/stations/<%= @station.id %>" method="POST">
<input type="hidden" name="_method" value="PUT"> 
<h1>Edit Station</h1>
<hr>
<fieldset>
<p><label>Station Name: </label> <input type="text" name="station[name]" value="<%=@station.name%>"/></p>
<p><label>Station Number: </label> <input type="text" name="station[number]" value="<%=@station.number%>"/></p>
<p><label>Free Bikes: </label> <input type="text" name="station[free]" value="<%=@station.free%>"/></p>
<p><label>Number of Bikes: </label> <input type="text" name="station[bikes]" value="<%=@station.bikes%>"/></p>
<p><label>Address: </label> <input type="text" name="station[address]" value="<%=@station.address%>"/></p>
<p><label>Latitude: </label> <input type="text" name="station[lat]" value="<%=@station.lat%>"/></p>
<p><label>Longitude: </label> <input type="text" name="station[lng]" value="<%=@station.lng%>"/></p>
</fieldset>
<input type="submit" value="Create"> or <a href="/stations">Cancel</a>